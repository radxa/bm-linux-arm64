#include "bm1684x_radxa_aicore.dtsi"

/ {
	model = "Radxa AICore BM1684x IO Board";
	/delete-node/ pka@58008000;
	/delete-node/ clp890@50018000;
	info {
		file-name = "bm1684x_radxa_aicore_io.dtb";
	};

	vcc_audio: audio-regulator {
		compatible = "regulator-fixed";
		enable-active-high;
		regulator-always-on;
		regulator-boot-on;
		gpio = <&port0a 7 GPIO_ACTIVE_HIGH>;
		regulator-name = "vcc_audio";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
	};

	gpio-leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";

		red-led {
			label = "red-led";
			gpios = <&port0a 8 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "none";
			default-state = "off";
		};

		green-led {
			label = "green-led";
			gpios = <&port0a 6 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "none";
			default-state = "off";
		};

		blue-led {
			label = "blue-led";
			gpios = <&port0a 5 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "heartbeat";
			default-state = "on";
		};
	};
};

&uart2 {
	status = "disabled";
};

&pwm{
	pinctrl-names = "default";
	pinctrl-0 = <&pwm0_acquire>;
};

&pwmfan {
	status = "okay";
	cooling-levels = <135 162 189 216 255>;
};

&soc_pwmfan_trip1 {
	temperature = <46000>; /* millicelsius */
};

&soc_pwmfan_trip2 {
	temperature = <49000>; /* millicelsius */
};

&soc_pwmfan_trip3 {
	temperature = <52000>; /* millicelsius */
};

&soc_pwmfan_trip4 {
	temperature = <55000>; /* millicelsius */
};

&soc_tpuclk440m_trip {
	temperature = <65000>;
};

&soc_tpuclk75m_trip {
	temperature = <95000>;
};

&soc_crit_trip {
	temperature = <105000>;
};

&vpu_reserved {
	reg = <0x3 0x40000000 0x0 0x08000000>; // 128M, for VPU firmware
	no-map;
};

&ion_for_vpu {
		compatible = "vpu-region";
		reg = <0x3 0x48000000 0x0 0x77359400>; // 1.86G
};

&ion_for_npu {
		compatible = "npu-region";
		reg = <0x1 0x24100000 0x1 0xdbf00000>; // 7.44G
};

&ion_for_vpp {
		compatible = "vpp-region";
		reg = <0x4 0x0 0x0 0x77359400>; // 1.86G, for VPP/JPU
};

&clock_init {
	clock-rates = \
		<2300000000>, <950000000>, <800000000>,

		<1000000000>, <1000000000>,
		<2000000000>, <50000000>,
		<100000000>, <100000000>,
		<125000000>, <50000000>,
		<25000000>, <125000000>,
		<50000000>, <25000000>,
		<500000000>, <25000000>,
		<1000000000>, <500000000>,
		<800000000>, <100000000>,
		<250000000>, <250000000>,
		<500000000>, <500000000>,
		<50000000>, <50000000>,
		<50000000>, <50000000>,
		<50000000>, <50000000>,
		<50000000>, <50000000>,
		<100000>, <100000>, <100000>,

		<1000000001>,
		<800000000>, <800000001>,
		<2300000001>, <575000000>;
};
